@model IEnumerable<IDQ_0__Personal_Secretary.Models.DataBase.Entities.Project>
@{
    //ViewData["Title"] = "Список Projects";
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <title>Магазин смартфонов</title>
</head>
<body>
    <h3>Смартфоны</h3>
    <table>
        <tr>
            <td>ID</td>
            <td>Priority</td> 
            <td>Name</td>
            <td>Descripiton</td>
            <td></td>
        </tr>
        @foreach (var project in Model){
        <tr>
            <td>@project.Id</td>
            <td>@project.Priority</td>
            <td>@project.Name</td>
            <td>@project.Description</td>
            <td><a href="~/Home/Buy/@project.Id">Купить</a></td>
        </tr>
        }
    </table>
</body>
</html>
